<Window x:Class="HashTableApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashTableApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="addBtn" Content="Add" HorizontalAlignment="Left" Height="42" Margin="138,244,0,0" VerticalAlignment="Top" Width="152" Click="addBtn_Click"/>
        <Button x:Name="removeBtn" Content="Remove" HorizontalAlignment="Left" Height="42" Margin="314,244,0,0" VerticalAlignment="Top" Width="152" Click="removeBtn_Click"/>
        <Button x:Name="showAllBtn" Content="Show ALL" HorizontalAlignment="Left" Height="42" Margin="515,244,0,0" VerticalAlignment="Top" Width="152" Click="showAllBtn_Click"/>
        <TextBox x:Name="keyTxt" HorizontalAlignment="Left" Height="42" Margin="138,116,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="328"/>
        <TextBox x:Name="valueTxt" HorizontalAlignment="Left" Height="42" Margin="138,163,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="328"/>

    </Grid>
</Window>
